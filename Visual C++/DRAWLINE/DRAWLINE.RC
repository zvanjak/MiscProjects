        MENUITEM "Hidden lines",                ID_HIDDEN_LINES
        MENUITEM SEPARATOR
        MENUITEM "Quadratic function",          ID_QUAD_FUNC
        MENUITEM "Cubic function",              ID_CUBIC_FUNC
        MENUITEM "Bezier function",             ID_BEZIER_FUNC
        MENUITEM SEPARATOR
        MENUITEM "Mandelbrot",                  ID_MANDELBROT
        MENUITEM "Julia",                       ID_JULIA
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Undo\tCtrl+Z",               ID_EDIT_UNDO
        MENUITEM SEPARATOR
