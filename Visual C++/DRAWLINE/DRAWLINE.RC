    POPUP "Exercises"
    BEGIN
        MENUITEM "Draw line",                   ID_EXERC_DRAWLINE
        MENUITEM SEPARATOR
        MENUITEM "Draw convex polygon",         ID_EXERC_DRAW_CONVEX_POLY
        MENUITEM "Draw polygon",                ID_EXERC_DRAW_POLY
